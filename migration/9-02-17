ALTER TABLE `brutypes` ADD `code` VARCHAR(255) NOT NULL AFTER `bruType`;

UPDATE `brutypes` SET `code`=REPLACE(`gradiApTableName`, '_ap', '') WHERE 1;

CREATE TABLE `calibration` ( `id` INT NOT NULL AUTO_INCREMENT ,
 `name` VARCHAR(255) NOT NULL ,
 `id_fdr` INT NOT NULL ,
 `id_user` INT NOT NULL ,
 `dt_created` TIMESTAMP NOT NULL DEFAULT CURRENT_TIMESTAMP ,
 `dt_updated` DATETIME NOT NULL ,
 PRIMARY KEY (`id`), INDEX (`id_fdr`),
 INDEX (`id_user`))
 ENGINE = InnoDB CHARSET=utf8 COLLATE utf8_general_ci;
